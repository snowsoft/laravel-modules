{
	"name" : "llama-laravel/modules",
	"description" : "Laravel Module Management",
	"keywords" : [
		"laravel",
		"llama",
		"module"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "XuaNguyen",
			"email" : "xuan.0211@gmail.com"
		}
	],
	"require" : {
		"php" : ">=5.6.4",
		"laravel/framework" : ">=5.3.*",
		"laravelcollective/html" : ">=5.3.*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4",
		"mockery/mockery" : "~0.9",
		"orchestra/testbench" : "^3.1|^3.2|^3.3",
		"phpro/grumphp" : "^0.9.1",
		"friendsofphp/php-cs-fixer" : "^1.11"
	},
	"autoload" : {
		"psr-4" : {
			"Llama\\Modules\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Llama\\Modules\\Tests\\" : "tests"
		}
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true
}